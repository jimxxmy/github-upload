<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.test.hibernate.bean.User" table="USER">
        <id column="ID" name="id" type="java.lang.Integer" />
        <property column="first_Num" name="firstNumber" type="java.lang.String" />
        <property column="second_Num" name="secondNumber" type="string" />
   
    </class>
</hibernate-mapping>
